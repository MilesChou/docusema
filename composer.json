{
    "name": "mileschou/docusema",
    "description": "The Schema docs generator",
    "type": "project",
    "license": "MIT",
    "keywords": [
        "markdown"
    ],
    "authors": [
        {
            "name": "MilesChou",
            "email": "jangconan@gmail.com",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/database": "^6"
    },
    "require-dev": {
        "mikey179/vfsstream": "^1.6",
        "phpstan/phpstan": "^0.11.8",
        "phpunit/phpunit": "^8",
        "psy/psysh": "^0.9.12",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "MilesChou\\Docusema\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
