{
    "name": "mileschou/docusema",
    "description": "The Schema docs generator",
    "type": "project",
    "license": "MIT",
    "keywords": [
        "schema"
    ],
    "authors": [
        {
            "name": "MilesChou",
            "email": "jangconan@gmail.com",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "doctrine/dbal": "^2.6",
        "illuminate/database": "^5.6 | ^6",
        "illuminate/events": "^5.6 | ^6",
        "illuminate/view": "^5.6 | ^6",
        "laravel-bridge/scratch": "^0.1.2",
        "mikey179/vfsstream": "^1.6",
        "symfony/console": "^4 | ^5",
        "vlucas/phpdotenv": "^3"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": "^7.5",
        "psy/psysh": "^0.9.12",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "MilesChou\\Docusema\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
