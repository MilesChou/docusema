{
    "name": "mileschou/schemarkdown",
    "description": "The schema document generator",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel", "database"
    ],
    "authors": [
        {
            "name": "MilesChou",
            "email": "jangconan@gmail.com",
            "homepage": "https://mileschou.github.io/",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "doctrine/dbal": "^2.6",
        "illuminate/console": "^5.6 | ^6",
        "illuminate/database": "^5.6 | ^6",
        "illuminate/events": "^5.6 | ^6",
        "illuminate/filesystem": "^5.6 | ^6",
        "illuminate/log": "^5.6 | ^6",
        "illuminate/view": "^5.6 | ^6",
        "laravel-bridge/scratch": "^0.5.2",
        "mikey179/vfsstream": "^1.6",
        "mileschou/codegener": "^1.0.2",
        "monolog/monolog": "^1.25",
        "symfony/yaml": "^4 | ^5",
        "vlucas/phpdotenv": "^3"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": "^7.5",
        "psy/psysh": "^0.9.12",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "MilesChou\\Schemarkdown\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
